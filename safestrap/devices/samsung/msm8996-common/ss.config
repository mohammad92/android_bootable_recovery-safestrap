# Device specific
SYSTEM=sda20
USERDATA=sda23
CACHE=sda21
BOOT=sda17
SYSTEM_FSTYPE=ext4
USERDATA_FSTYPE=ext4
SS_FSTYPE=ext4
SS_PART=sda23
SS_DIR=/ss/safestrap
HIJACK_BIN=e2fsck
HIJACK_LOC=bin
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/bbx cat /proc/bootinfo | /sbin/bbx grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/devices/soc/900000.qcom\x2cmdss_mdp/900000.qcom\x2cmdss_mdp:qcom\x2cmdss_fb_primary/leds/lcd-backlight/brightness
BACKLIGHT_BRIGHTNESS_VALUE=162
TASKSET_CPUS=0-3
SS_USE_DATAMEDIA=1
SS_COPY_SYSTEM_DATA=1
DEBUG_MODE=0